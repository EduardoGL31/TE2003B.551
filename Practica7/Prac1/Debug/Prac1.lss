                                 ;----------------------------STACK POINTER-------------------------------------------
000000 e004                      LDI R16, HIGH(RAMEND)		;Loads an 8-bit constant directly to register 16 to 31.
000001 bf0e                      OUT SPH, R16				;Stores data from register Rr in the Register File to I/O Space (Ports, Timers, Configuration Registers,etc.).
000002 e50f                      LDI R16, LOW(RAMEND)		;Loads an 8-bit constant directly to register 16 to 31.
000003 bf0d                      OUT SPL, R16				;Stores data from register Rr in the Register File to I/O Space (Ports, Timers, Configuration Registers,etc.).
